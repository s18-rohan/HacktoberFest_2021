#include<iostream>
#include<array>
using namespace std;
int main()
{
   	//array<object_type, array_size> array_name;

	array<int,8> a = {1,2,3,4,5,6,7,8};
    array<int,8> b = {8,7,6,5,4,3,2,1};
    
    a.swap(b);  // swaps array b to a
    
    cout << "a is : ";
    for(int i=0; i < 8; i++) {
    cout << a[i] <<" ";
    }
    cout << endl;
	
    cout << "b is : ";
    for(int i=0; i < 8; i++) {
    cout << a[i] <<" ";
    }
	cout<<endl;
	
	cout << "ON position 2 element present is "<<a.at(2)<<endl;     // print the element at index 2
    cout << "ON position 4 element present is "<<b.at(4)<<endl;     // print the element at index 4

	return 0;
		
}
